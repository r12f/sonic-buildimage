[Unit]
Description=Dash engine service
Before=syncd.service
StartLimitIntervalSec=1200
StartLimitBurst=3

[Service]
User=root
ExecStartPre=/usr/local/bin/dash_engine.sh start
ExecStart=/usr/local/bin/dash_engine.sh wait
ExecStop=/usr/local/bin/dash_engine.sh stop
Restart=always
RestartSec=30

[Install]
WantedBy=multi-user.target
